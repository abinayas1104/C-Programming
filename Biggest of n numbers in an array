#include<stdio.h>
#include<conio.h>
int main()
{
int n,a[100],i,big;
clrscr();
printf("enter the size of the array");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("enter the %d element:,i+1);
scanf("%d",&a[i]);
}
big=a[0];
for(i=1;i<n;i++)
{
if(big<a[i]);
{
big=a[i];
}
}
printf("The biggest number in the array is %d",big);
getch();
return 0;
}
